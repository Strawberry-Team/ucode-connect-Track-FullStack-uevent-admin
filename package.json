{
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start:dev": "dotenv -v NODE_ENV=development -e .env.development -- nest start --watch",
    "start:dev:local": "dotenv -v NODE_ENV=development -e .env.development.local -- nest start --watch",
    "start:debug": "dotenv -v NODE_ENV=development -e .env.development -- nest start --debug --watch",
    "start:prod": "dotenv -v NODE_ENV=production -e .env.production -- node dist/main.js",
    "deploy": "npm run build && npm run start:prod",
    "docker:build": "docker build -t admin-panel .",
    "docker:run": "docker run -p 8000:8000 --env-file .env admin-panel",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.0",
    "@adminjs/nestjs": "^6.1.0",
    "@adminjs/sql": "^2.2.6",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cookie-session": "^2.0.49",
    "@types/session-file-store": "^1.2.5",
    "adminjs": "^7.7.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.0",
    "dotenv": "^16.6.0",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "mysql2": "latest",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "session-file-store": "^1.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "11.0.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "name": "uevent-admin",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
